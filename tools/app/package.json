{
  "name": "@enterprise/app-cli",
  "version": "1.0.0",
  "description": "CLI for managing EnterpriseAppBase development environment and API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "app": "./bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node bin/app.js",
    "clean": "rimraf dist",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:cov": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "enterprise",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.23",
    "open": "^10.1.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.14.0",
    "@types/validator": "^13.12.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "memfs": "^4.6.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

# Test Environment Configuration
NODE_ENV=test

# Test Database (use separate test database!)
# Individual connection parameters (preferred method)
POSTGRES_HOST=localhost
POSTGRES_PORT=5433
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=enterprise_app_test
POSTGRES_SSL=false

# Legacy DATABASE_URL (kept for backward compatibility with direct Prisma CLI usage)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/enterprise_app_test

# JWT Configuration
JWT_SECRET=test-secret-key-minimum-32-characters-long
JWT_ACCESS_TTL_MINUTES=15
JWT_REFRESH_TTL_DAYS=14

# Google OAuth (mocked in tests)
GOOGLE_CLIENT_ID=test-client-id
GOOGLE_CLIENT_SECRET=test-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3000/api/auth/google/callback

# Admin Bootstrap
INITIAL_ADMIN_EMAIL=admin@example.com

# App URL
APP_URL=http://localhost:3000

# Disable telemetry in tests
OTEL_ENABLED=false

{
  "name": "api",
  "version": "1.0.0",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linter configured'",
    "test": "jest --config ./test/jest.config.js --testPathIgnorePatterns='e2e'",
    "test:all": "jest --config ./test/jest.config.js",
    "test:watch": "jest --config ./test/jest.config.js --watch --testPathIgnorePatterns='e2e'",
    "test:cov": "jest --config ./test/jest.config.js --coverage --testPathIgnorePatterns='e2e'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest.config.js --testPathIgnorePatterns='e2e'",
    "test:e2e": "jest --config ./test/jest.config.js --testRegex '.e2e.spec.ts$'",
    "test:unit": "jest --config ./test/jest.config.js --testPathIgnorePatterns='e2e'",
    "test:ci": "jest --config ./test/jest.config.js --coverage --ci --reporters=default --reporters=jest-junit --testPathIgnorePatterns='e2e'",
    "prisma": "node scripts/prisma-env.js",
    "prisma:generate": "node scripts/prisma-env.js generate",
    "prisma:migrate": "node scripts/prisma-env.js migrate deploy",
    "prisma:migrate:dev": "node scripts/prisma-env.js migrate dev",
    "prisma:studio": "node scripts/prisma-env.js studio",
    "prisma:seed": "node scripts/prisma-env.js db seed",
    "prisma:push": "node scripts/prisma-env.js db push",
    "db:test": "node scripts/test-db-connection.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.986.0",
    "@aws-sdk/lib-storage": "^3.986.0",
    "@aws-sdk/s3-request-presigner": "^3.986.0",
    "@databricks/sql": "^1.12.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.4.0",
    "@fastify/static": "^9.0.0",
    "@langchain/anthropic": "^1.3.16",
    "@langchain/core": "^1.1.19",
    "@langchain/langgraph": "^1.1.4",
    "@langchain/openai": "^1.2.6",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.12",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.13",
    "@nestjs/schedule": "^6.1.1",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/terminus": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.211.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@prisma/client": "^5.x",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "js-yaml": "^4.1.1",
    "mssql": "^12.2.0",
    "mysql2": "^3.16.3",
    "neo4j-driver": "^6.0.1",
    "nestjs-zod": "^3.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "pino": "^9.14.0",
    "pino-opentelemetry-transport": "^1.1.0",
    "reflect-metadata": "^0.2.x",
    "rxjs": "^7.x",
    "snowflake-sdk": "^2.3.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.13",
    "@swc/cli": "^0.8.0",
    "@swc/core": "^1.15.11",
    "@types/jest": "^29.x",
    "@types/js-yaml": "^4.0.9",
    "@types/mssql": "^9.1.9",
    "@types/node": "^20.x",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.x",
    "dotenv": "^16.4.5",
    "jest": "^29.x",
    "jest-junit": "^16.x",
    "jest-mock-extended": "^3.x",
    "passport-custom": "^1.x",
    "pino-pretty": "^13.1.3",
    "prisma": "^5.x",
    "supertest": "^7.2.2",
    "ts-jest": "^29.x",
    "ts-node": "^10.x",
    "typescript": "^5.x"
  },
  "prisma": {
    "seed": "ts-node --project prisma/tsconfig.json prisma/seed.ts"
  }
}
